        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".QuizCard01"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".QuizCard02"
            android:parentActivityName=".QuizCard01">
            >
        </activity>
        <activity android:name=".QuizCard03" />
        <activity android:name=".QuizCard04" />
        <activity android:name=".QuizCard05" />
        <activity android:name=".Result" />
        <activity android:name=".QuizCard06" />
        <activity android:name=".QuizCard07"></activity>
    </application>

</manifest>